{
  "name": "Angles",
  "version": "1.0",
  "description": "Check your bias!",
  "manifest_version": 2,
  "permissions": [],
  "background": {
    "scripts": ["jquery-3.5.1.min.js", "allsides.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["jquery-3.5.1.min.js", "content.js"],
      "css": ["content.css", "popup.css"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "browser_action":{
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
  
}