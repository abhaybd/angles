{
  "name": "Angles",
  "version": "1.0",
  "description": "Check your bias!",
  "manifest_version": 2,
  "permissions": ["declarativeContent"],
  "background": {
    "scripts": ["jquery-3.5.1.min.js", "allsides.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["jquery-3.5.1.min.js", "content.js"],
      "css": ["content.css"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "browser_action":{
    "default_popup": "popup.html"
  }
}